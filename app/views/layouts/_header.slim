                            a href="blog-detail-sidebar.html"  With Sidebar
                          li
                            a href="blog-detail-no-sidebar.html"  Without Sidebar
                  li
                    a href="faq-with-sidebar.html"
                      | FAQs
                  li
                    a href="contact-page.html"
                      | Contact
              /! navbar-collapse
            .col-md-4.col-sm-4.col-xs-9.hidden-xs
              .icons-section.pad-top20.pull-right
                ul
                  li.search-box1.hidden-xs
                    .search-box-top-bar
                      input.search_terms name="search" placeholder="Search" type="text" /
                      button.button-style type="submit" value=""
