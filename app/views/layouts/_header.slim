  /! //==mega menu start==//
  #main-menu.wa-main-menu
    /! Menu
    .wathemes-menu.relative
      /! navbar
      .navbar.navbar-default.navbar-bg-dark role="navigation"
        .container
          .row
            .col-md-8.col-sm-8.col-xs-12
              .navbar-header.pad-top15.pad-bottom15
                /! Button For Responsive toggle
                button.navbar-toggle data-target=".navbar-collapse" data-toggle="collapse" type="button"
                  span.sr-only Toggle navigation
                  span.icon-bar
                  span.icon-bar
                  span.icon-bar
                .icons-section.boxx.pull-right
                  ul
                    li.hidden-sm.hidden-md.hidden-lg.hover-mini-cart
                      a.mini-cart-icon href="#"
                        i.fa.fa-shopping-cart aria-hidden="true"
                      .mini-card-section
                        .mini-cart-product-box.pad-top20
                          .cart-img-box
                            figure
                              img alt="" src=asset_path("all/mini-cart-img1.jpg")
                          .cart-price-box
                            h5
                              a href="product-detail.html"  Modern Chairs
                            p $100x1
                          .cancle-box
                            a href="#"  x
                        .mini-cart-product-box.pad-top20
                          .cart-img-box
                            figure
                              img alt="" src=asset_path("all/mini-cart-img1.jpg")
                          .cart-price-box
                            h5
                              a href="product-detail.html"  Modern Chairs
                            p $100x1
                          .cancle-box
                            a href="#"  x
                        .price-text-sec.pull-right.pad-top10
                          p
                            | Total $200
                          p
                        .clear
                        .price-button-box.pad-top10.pad-bottom20.pull-right
                          a.itg-button.View-btns href="cart.html"  View cart
                          a.itg-button.pull-right.Checkout-btns href="checkout.html"  Checkout
                a.navbar-brand.hidden-sm.hidden-md.hidden-lg.logo-second href="index.html"
                  img.site_logo alt=("Site Logo") src=asset_path("menu-logo.png")
              /! Navbar Collapse
              .navbar-collapse.collapse
                /! nav
                ul.nav.navbar-nav
                  li
                    a href="/" = t(".home")
                  li
                    a href="/products" = t(".products")
                  li
                    a href="#"  Blog
                    ul.dropdown-menu
                      li
                        a href="blog-grid-style1.html"  Blogs Style 1
